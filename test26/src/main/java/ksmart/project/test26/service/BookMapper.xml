<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ksmart.project.test26.service.BookMapper">
 	<!-- Book 리스트 조회하는 쿼리문 -->
  <select id="selectBookList"
  			resultType="ksmart.project.test26.service.Book">
  			
  	 	SELECT
			book_id as bookId,
			book_name as bookName
		FROM test.book
 	
  </select>
  
  <!-- Book 입력하기 위한 쿼리문 -->
  <insert id="insertBook"
  			parameterType="ksmart.project.test26.service.Book">
 	 INSERT INTO test.book (book_name) 
		VALUES (#{bookName})
  </insert>
</mapper>